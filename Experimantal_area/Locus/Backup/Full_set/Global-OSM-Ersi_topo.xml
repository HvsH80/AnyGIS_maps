	<provider id="8888002" type="0" visible="true" background="-1">
		<name>Global - OSM</name>
		<mode>ESRI Topo</mode>
		<countries>World</countries>
		<usage>hiking</usage>
		<url><![CDATA[https://services.arcgisonline.com/ArcGIS/rest/services/World_Topo_Map/MapServer/tile/{z}/{y}/{x}]]></url>
