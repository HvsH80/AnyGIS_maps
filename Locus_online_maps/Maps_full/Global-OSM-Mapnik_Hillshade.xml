    <mode>Mapnik Proxy</mode>
    <countries>World</countries>
    <usage>city,hiking</usage>
    <url><![CDATA[https://anygis.herokuapp.com/Osm_Mapnik_proxy/{x}/{y}/{z}]]></url>
