<providers>

	<provider id="10009" type="1" visible="true" background="-1">
        <name>Europe - Eastern</name>
        <mode>Ukraine - Visicom</mode>
        <countries>Ukraine</countries>
        <url><![CDATA[http://tms{s}.visicom.ua/2.0.0/planet3/base/{z}/{x}/{y}.png]]></url>
        <serverPart>0;1;2;3</serverPart>
        <zoomPart>{z}-8</zoomPart>
        <zoomMin>1</zoomMin>
        <zoomMax>28</zoomMax>
        <tileSize>256</tileSize>
        <usage>City</usage>	
        <extraHeader><![CDATA[User-Agent#Mozilla/5.0 (Windows NT 6.1; WOW64; rv:7.0.1) Gecko/20100101 Firefox/7.0.1]]></extraHeader>
        <extraHeader><![CDATA[Referer#http://maps.google.com/]]></extraHeader>
    </provider>

	
