<providers>

	<provider id="9999007" type="0" visible="true" background="-1">
		<name>Global - Satellites</name>
		<mode>All Satellites</mode>
		<countries>World</countries>
		<usage>photo</usage>
		<url><![CDATA[https://anygis.herokuapp.com/Combo_All_Satellites/{x}/{y}/{z}]]></url>
		<zoomPart>{z}-8</zoomPart>
		<zoomMin>8</zoomMin>
		<zoomMax>30</zoomMax> <!-- Max: 22+8 -->
		<tileSize>256</tileSize>
		<attribution><![CDATA[© AnyGIS]]></attribution>
		<extraHeader><![CDATA[User-Agent#Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/57.0.2987.110 Safari/537.36]]></extraHeader>
		<extraHeader><![CDATA[Referer#https://www.openstreetmap.org/]]></extraHeader>
	</provider>


