<providers>

	<provider id="10081" type="2" visible="true" required="true" background="-1">
		<name>Global - Satellites</name>
		<mode>Yandex Satellite</mode>
		<countries>Russia</countries>
		<usage>photo</usage>
		<url><![CDATA[https://sat{s}.maps.yandex.net/tiles?l=sat&x={x}&y={y}&z={z}]]></url>
		<serverPart>01;02;03;04</serverPart>
		<zoomPart>{z}-8</zoomPart>
		<zoomMin>8</zoomMin>
		<zoomMax>27</zoomMax> <!-- Max: 19+8 -->
		<tileSize>256</tileSize>
		<attribution><![CDATA[© Яндекс & others]]></attribution>
		<extraHeader><![CDATA[User-Agent#Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/57.0.2987.110 Safari/537.36]]></extraHeader>
		<extraHeader><![CDATA[Referer#https://maps.yandex.ru/]]></extraHeader>
	</provider>

</providers>