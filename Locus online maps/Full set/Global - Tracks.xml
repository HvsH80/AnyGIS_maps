<providers>

	<provider id="10650" type="0" visible="true" background="-1">
		<name>Global - Tracks</name>
		<mode>OpenStreetMap</mode>
		<countries>World</countries>
		<usage>overlay</usage>
		<url><![CDATA[http://gps-{s}.tile.openstreetmap.org/lines/{z}/{x}/{y}.png]]></url>
		<serverPart>a;b;c</serverPart>
		<zoomPart>{z}-8</zoomPart>
		<zoomMin>8</zoomMin>
		<zoomMax>27</zoomMax> <!-- Max: 19+8 -->
		<tileSize>256</tileSize>
		<attribution><![CDATA[Map Data <a href="http://www.openstreetmap.org/">OpenStreetMap</a>, <a href="http://creativecommons.org/licenses/by-sa/2.0/">CC-BY-SA</a>]]></attribution>
		<extraHeader><![CDATA[User-Agent#Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/57.0.2987.110 Safari/537.36]]></extraHeader>
	</provider>




